{
  "name": "it_p_launcher",
  "version": "1.0.0",
  "private": true,
  "description": "Launcher for IT-PROCESSES by Yasin Yazici (CEO)",
  "main": "main.js",
  "scripts": {
    "build": "Xcopy /D /I /E /Y \"./assets\" \"./dist/assets\" && tsc",
